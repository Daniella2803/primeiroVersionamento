algoritmo "semnome"
//Pedir para o usuário digitar um número e imprimir
//este número na tela.
var
   numero:inteiro
   resultado:caractere
inicio
procedimento resultado(numero:inteiro)
inicio
   escreval("O numero digitado foi: ",numero)
fimprocedimento
escreval("Informe um numero: ")
leia(numero)
resultado(numero)
fimalgoritmo